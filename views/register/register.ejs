<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>GrowUp Portfolio</title>

    <!-- Font Icon -->
    <link rel="stylesheet" href="fonts/material-icon/css/material-design-iconic-font.min.css">

    <!-- Main css -->
    <link rel="icon" type="favicon" href="images/favicon.png">
    <link rel="stylesheet" href="css/registeration.css">
    <link rel="stylesheet" href="css/regstyle.css">
</head>
<script>
    var url = 'https://wati-integration-service.clare.ai/ShopifyWidget/shopifyWidget.js?49650';
    var s = document.createElement('script');
    s.type = 'text/javascript';
    s.async = true;
    s.src = url;
    var options = {
  "enabled":true,
  "chatButtonSetting":{
      "backgroundColor":"#4dc247",
      "ctaText":"",
      "borderRadius":"25",
      "marginLeft":"0",
      "marginBottom":"50",
      "marginRight":"50",
      "position":"right"
  },
  "brandSetting":{
      "brandName":"Growup Tech Solutions",
      "brandSubTitle":"Contact our support",
      "brandImg":"http://growupinfo.com/assets/images/logo-icon.jpeg",
      "welcomeText":"Hi there!\nHow can I help you?",
      "messageText":"Hello, I have a question about growup portfolio",
      "backgroundColor":"#0a5f54",
      "ctaText":"Start Chat",
      "borderRadius":"25",
      "autoShow":false,
      "phoneNumber":"923365453183"
  }
};
    s.onload = function() {
        CreateWhatsappChatWidget(options);
    };
    var x = document.getElementsByTagName('script')[0];
    x.parentNode.insertBefore(s, x);
</script>
<body class="img">
    <div class="main">

        <!-- Sign up form -->
        <section class="signup">
            <h3 style="text-align: center; width: 100%; font-size: 26px;" class="topdown growreg"> <span style="font-size: 38px;">G</span>rowup <span style="font-size: 38px;">P</span>ortfolio</h3>
            <div class="container">

                <div class="signup-content">

                    <div class="signup-form">

                        <h2 class="form-title">Sign up</h2>
                        <form method="POST" class="register-form" id="register-form">
                            <div class="form-group">
                                <label for="name"><i class="zmdi zmdi-account material-icons-name"></i></label>
                                <input type="text" name="name" id="name" placeholder="Your Name" required/>
                            </div>
                            <div class="form-group">
                                <label for="email"><i class="zmdi zmdi-email"></i></label>
                                <input type="email" name="email" id="email" placeholder="Your Email" required />
                            </div>
                            <div class="form-group">
                                <label for="pass"><i class="zmdi zmdi-lock"></i></label>
                                <input type="password" name="pass" id="pass" placeholder="Password" pattern="(?=.*[A-Z]).{8,}" title="Must contain at least one uppercase letter and at least 8 or more characters" required/>
                            </div>
                            <div class="form-group">
                                <label for="re-pass"><i class="zmdi zmdi-lock-outline"></i></label>
                                <input type="password" name="re_pass" id="re_pass" placeholder="Repeat your password" pattern="(?=.*[A-Z]).{8,}" title="Must contain at least one uppercase letter and at least 8 or more characters" required/>
                            </div>
                            <div class="form-group">
                                <input type="checkbox" name="agree-term" id="agree-term" class="agree-term"/>
                                <label for="agree-term" id="label-agree-term" class="label-agree-term"><span><span></span></span>I agree all statements in  <a href="http://growupinfo.com/Privacy.html" class="term-service">Terms of service</a></label>
                            </div>
                            <div class="form-group form-button">
                                <input type="submit" name="signup" id="signup" class="form-submit" value="Register"/>
                            </div>
                            <h1><%= wrong %></h1>
                        </form>
                    </div>
                    <div class="signup-image">
                        <figure><img class="vectorImage" src="images/signup-image.png" alt="sing up image"></figure>
                        <a href="/login" class="signup-image-link"><h3>I am already member</h3></a>
                    </div>
                </div>
            </div>
        </section>
        
       

    </div>

    <!-- JS -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="js/regmain.js"></script>
</body><!-- This templates was made by GrowUp  -->
</html>